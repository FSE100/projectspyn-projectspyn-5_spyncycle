global key
InitKeyboard();
while 1
    pause(0.1);
    switch key
        case 'd'
            brick.ResetMotorAngle('C');
            brick.MoveMotorAngleAbs('C', 2, 17.5, 'Brake');
            brick.WaitForMotor('C');
        case 'l'
            brick.ResetMotorAngle('C');
            brick.MoveMotorAngleAbs('C', 5, -25, 'Brake');
            brick.WaitForMotor('C');
        case'q'
            break;
    end
end
CloseKeyboard();
